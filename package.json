{
  "name": "algobot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3006",
    "build": "next build",
    "start": "next start -p 3006",
    "start:prod": "NODE_ENV=production next start -p 3006",
    "lint": "eslint",
    "server": "node backend/server.js",
    "server:dev": "NODE_ENV=development nodemon backend/server.js",
    "server:prod": "NODE_ENV=production node backend/server.js",
    "build:prod": "NODE_ENV=production next build",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "prod:full": "npm run build:prod && concurrently \"npm run server:prod\" \"npm run start:prod\""
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "framer-motion": "^12.24.10",
    "helmet": "^8.1.0",
    "mongoose": "^9.1.2",
    "morgan": "^1.10.1",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "nodemon": "^3.1.11"
  }
}
